#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x128e08de0 .scope module, "tb_counter" "tb_counter" 2 1;
 .timescale 0 0;
v0x128e1eae0_0 .var "clk", 0 0;
v0x128e1eb80_0 .net "count", 3 0, v0x128e1e880_0;  1 drivers
v0x128e1ec30_0 .var "rst", 0 0;
v0x128e1ed00_0 .var "up_down", 0 0;
S_0x128e07590 .scope module, "my_counter" "counter4" 2 8, 3 1 0, S_0x128e08de0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "up_down";
    .port_info 3 /OUTPUT 4 "q";
v0x128e08f50_0 .net "clk", 0 0, v0x128e1eae0_0;  1 drivers
v0x128e1e880_0 .var "q", 3 0;
v0x128e1e930_0 .net "rst", 0 0, v0x128e1ec30_0;  1 drivers
v0x128e1e9e0_0 .net "up_down", 0 0, v0x128e1ed00_0;  1 drivers
E_0x128e09350 .event posedge, v0x128e08f50_0;
    .scope S_0x128e07590;
T_0 ;
    %wait E_0x128e09350;
    %load/vec4 v0x128e1e930_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x128e1e880_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x128e1e9e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x128e1e880_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x128e1e880_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x128e1e880_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x128e1e880_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x128e08de0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x128e1eae0_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x128e1eae0_0;
    %inv;
    %store/vec4 v0x128e1eae0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x128e08de0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x128e1ec30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x128e1ed00_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x128e1ec30_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x128e1ed00_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x128e08de0;
T_3 ;
    %wait E_0x128e09350;
    %vpi_call 2 32 "$display", "Time = %0t |rst=%b| up_down = %b | count = %b (%0d)", $time, v0x128e1ec30_0, v0x128e1ed00_0, v0x128e1eb80_0, v0x128e1eb80_0 {0 0 0};
    %jmp T_3;
    .thread T_3;
    .scope S_0x128e08de0;
T_4 ;
    %vpi_call 2 36 "$dumpfile", "tb_counter.vcd" {0 0 0};
    %vpi_call 2 37 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x128e08de0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
